import shutil
from os.path import isfile, join

def move_symbols(symbols, dest):
    for s in symbols:
        filename = '{}.csv'.format(s)
        get_ipython().system('mkdir {dest}/{s}')
        shutil.move(join('hist', filename), join(dest,'{}'.format(s), 'data.csv'))

